{
  "name": "narokishi/lightweight-di",
  "description": "Lightweight dependency injection with lazy load for PHP applications",
  "keywords": ["php", "lightweight-di", "dependency-injection", "lazy-load-di"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Tomasz Motyl",
      "email": "tomasz.motyl806@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.0"
  },
  "autoload": {
    "psr-4": {
      "Narokishi\\DependencyInjection\\": "src/DependencyInjection"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Narokishi\\DependencyInjection\\Tests\\": "tests/DependencyInjection"
    }
  },
  "require-dev": {
    "php-coveralls/php-coveralls": "^2.1",
    "phpunit/phpunit": "^7.3.2"
  }
}
